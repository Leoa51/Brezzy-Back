#POSTEGRE ENVIRONMENT VARIABLES
# PostgreSQL database connection settings
POSTGRES_USER=your_username
POSTGRES_PASSWORD=your_password
POSTGRES_DB=your_database_name
POSTGRES_PORT=5432
POSTEGRE_VERSION=17
POSTEGRE_DATABASE_URL="postgresql://your_username:your_password@localhost:5432/your_database_name?schema=public"
